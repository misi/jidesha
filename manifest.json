{
  "manifest_version": 2,
  "name": "Randevu Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with Randevu (Jitsi Meet and Jitsi Videobridge).",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "randevu-logo-16x16.png",
    "48": "randevu-logo-48x48.png",
    "128": "randevu-logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://randevu.lab.vvc.niif.hu/*"
      ]
  }
}
